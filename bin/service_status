#!/bin/sh
# 查看 apiService-running
apiService_user=$(ps -ef | grep "/apiService-running" | grep -v "grep" | awk '{print $1}')
apiService_starttime=$(ps -ef | grep "/apiService-running" | grep -v "grep" | awk '{print $5}')
apiService_pid=$(ps -ef | grep "/apiService-running" | grep -v "grep" | awk '{print $2}')
apiService_bin=$(ps -ef | grep "/apiService-running" | grep -v "grep" | awk '{print $8}')
if [ "$apiService_pid" ]; then
  printf "用户：$apiService_user   启动时间：$apiService_starttime 进程ID：$apiService_pid  执行路径：$apiService_bin\n"
fi

# 查看 huobiService-running
huobiService_user=$(ps -ef | grep "/huobiService-running" | grep -v "grep" | awk '{print $1}')
